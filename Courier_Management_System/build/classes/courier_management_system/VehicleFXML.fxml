<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="courier_management_system.VehicleFXMLController">
   <children>
      <Button fx:id="AddVehicle" contentDisplay="TOP" layoutX="130.0" layoutY="200.0" mnemonicParsing="false" onAction="#AddVehicle" prefHeight="150.0" prefWidth="150.0" text="Add new vehicle">
         <graphic>
            <ImageView fx:id="imageview" fitHeight="62.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/plus.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="UpdateVehicle" contentDisplay="TOP" layoutX="375.0" layoutY="200.0" mnemonicParsing="false" onAction="#UpdateVehicle" prefHeight="150.0" prefWidth="150.0" text="Update vehicle info">
         <graphic>
            <ImageView fx:id="imageview2" fitHeight="62.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/update-arrows.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="ShowVehicle" contentDisplay="TOP" layoutX="618.0" layoutY="200.0" mnemonicParsing="false" onAction="#ShowVehicle" prefHeight="150.0" prefWidth="150.0" text="Show vehicle details">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <graphic>
            <ImageView fitHeight="62.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/view-details.png" />
               </image></ImageView>
         </graphic>
      </Button>
      <Button layoutX="21.0" layoutY="14.0" mnemonicParsing="false" onAction="#back_button" prefHeight="50.0" prefWidth="50.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/arrow-go-back-line.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
